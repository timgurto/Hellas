<root>

<item id="santaHat" name="Winter Hat" ilvl="1" class="woolHead" gearSlot="head" sounds="fabric" bind="pickup" >
    <offset x="-3" y="-8" />
    <particles profile="snow" x="1" y="36" />
</item>

<objectType id="xmasGift2019a" name="Winter Gift"
    xDrawOffset="-7" yDrawOffset="-10" imageFile="gift"
