    allowedTerrain="fertile" >
    <yield id="charcoal" />
    <collisionRect x="-7" y="-3" w="15" h="6" />
    <particles profile="snow" />
</objectType>
<objectType id="xmasGift2019b" name="Winter Gift"
    xDrawOffset="-7" yDrawOffset="-10" imageFile="gift"
    canGather="1" gatherTime="10000"
    allowedTerrain="fertile" >
    <yield id="santaHat" />
    <collisionRect x="-7" y="-3" w="15" h="6" />
